{
  "name": "examensradar",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "wrangler dev",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply examensradar-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply examensradar-db --remote",
    "db:seed:local": "wrangler d1 execute examensradar-db --local --file=./src/db/seed.sql",
    "db:seed:remote": "wrangler d1 execute examensradar-db --remote --file=./src/db/seed.sql",
    "deploy": "wrangler deploy"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-start": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "@trpc/client": "^11.8.1",
    "@trpc/react-query": "^11.8.1",
    "@trpc/server": "^11.8.1",
    "better-auth": "^1.4.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.561.0",
    "nanoid": "^5.1.6",
    "nitro": "npm:nitro-nightly@latest",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "superjson": "^2.2.6",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.6",
    "tw-animate-css": "^1.3.6",
    "vite-tsconfig-paths": "^6.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@cloudflare/workers-types": "^4.20260118.0",
    "@tanstack/devtools-vite": "^0.3.11",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "drizzle-kit": "^0.31.8",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "wrangler": "^4.59.2"
  }
}
